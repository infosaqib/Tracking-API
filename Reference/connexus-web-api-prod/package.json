{
  "name": "connexus-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:with-sourcemaps": "npm run build && npm run sentry:sourcemaps",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/connexus-be/main",
    "start:connexus": "nest start connexus-be --watch",
    "start:sow": "nest start sow-rfq --watch",
    "start:background-jobs": "nest start background-jobs --watch",
    "dev": "concurrently --kill-others \"yarn start:connexus\" \"yarn start:sow\" --prefix-colors \"blue.bold,green.bold\" --names \"connexus,sow-rfq\" --prefix \"[{name}]\"",
    "dev:selective": "concurrently --kill-others-on-fail --prefix-colors \"yellow.bold,magenta.bold,cyan.bold,red.bold,green.bold\" --names \"$npm_config_app1,$npm_config_app2,$npm_config_app3,$npm_config_app4,$npm_config_app5\" --prefix \"[{name}]\" \"if [ ! -z $npm_config_app1 ]; then yarn start $npm_config_app1 --watch; else echo 'No app1 specified'; fi\" \"if [ ! -z $npm_config_app2 ]; then yarn start $npm_config_app2 --watch; else echo 'No app2 specified'; fi\" \"if [ ! -z $npm_config_app3 ]; then yarn start $npm_config_app3 --watch; else echo 'No app3 specified'; fi\" \"if [ ! -z $npm_config_app4 ]; then yarn start $npm_config_app4 --watch; else echo 'No app4 specified'; fi\" \"if [ ! -z $npm_config_app5 ]; then yarn start $npm_config_app5 --watch; else echo 'No app5 specified'; fi\"",
    "dev:apps": "node scripts/run-apps.js",
    "lint:all": "eslint \"{src,apps,libs,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "lint": "eslint --cache \"{src,apps,libs,test}/**/*.ts\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/connexus-be/test/jest-e2e.json",
    "prepare": "husky",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "npx prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed",
    "prisma:generate": "npx prisma generate",
    "prisma:reset": "npx prisma migrate reset && npx prisma seed",
    "script:seed": "ts-node scripts/index.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org connexus-ox --project connexus-web-api ./dist && sentry-cli sourcemaps upload --org connexus-ox --project connexus-web-api ./dist",
    "docker-start:connexus": "npm run migrate:deploy && npm run sentry:sourcemaps && node dist/apps/connexus-be/main",
    "docker-start:sow": "npm run migrate:deploy && node dist/apps/sow-rfq/main",
    "docker-start:background-jobs": "node dist/apps/background-jobs/main",
    "commit": "cz"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.738.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.625.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/client-ses": "^3.632.0",
    "@aws-sdk/client-sesv2": "^3.629.0",
    "@aws-sdk/client-sqs": "^3.854.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "@azure/openai": "^2.0.0",
    "@casl/ability": "^6.7.3",
    "@casl/prisma": "^1.5.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^6.11.1",
    "@prisma/extension-optimize": "^1.1.8",
    "@prisma/instrumentation": "^6.3.0",
    "@scalar/nestjs-api-reference": "^0.4.6",
    "@sentry/cli": "^2.36.6",
    "@sentry/nestjs": "^8.33.0",
    "@sentry/profiling-node": "^8.33.0",
    "@ssut/nestjs-sqs": "^3.0.1",
    "@types/pdfkit": "^0.17.2",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "csv-stringify": "^6.5.2",
    "date-fns": "^4.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nest-winston": "^1.10.0",
    "nestjs-prisma": "^0.25.0",
    "node-fetch": "2",
    "npm": "^10.8.2",
    "openai": "^5.9.2",
    "pdfkit": "^0.16.0",
    "pdfmake": "^0.2.20",
    "prisma": "^6.11.1",
    "prisma-extension-pagination": "^0.7.5",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^10.0.0",
    "winston": "^3.13.1",
    "winston-cloudwatch": "^6.3.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-s3-transport": "^2.0.10",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "@swc/jest": "^0.2.36",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.3.1",
    "@types/pdfmake": "^0.2.11",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-prisma": "^0.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "prisma-dbml-generator": "^0.12.0",
    "semantic-release": "^24.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.99.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s?$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/guards(|/.*)$": "<rootDir>/libs/guards/src/$1",
      "^@app/ability(|/.*)$": "<rootDir>/libs/ability/src/$1",
      "^@app/s3(|/.*)$": "<rootDir>/libs/s3/src/$1",
      "^@app/export-data(|/.*)$": "<rootDir>/libs/export-data/src/$1",
      "^@app/health(|/.*)$": "<rootDir>/libs/health/src/$1"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
