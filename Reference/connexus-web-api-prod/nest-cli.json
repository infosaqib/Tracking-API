{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/connexus-be/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true,
    "assets": [
      {
        "include": "**/*.hbs",
        "watchAssets": true,
        "outDir": "dist/apps/connexus-be"
      },
      {
        "include": "libs/**/*.hbs",
        "watchAssets": true,
        "outDir": "dist/apps/connexus-be"
      },
      {
        "include": "**/*.png",
        "watchAssets": true,
        "outDir": "dist/apps/connexus-be"
      },
      {
        "include": "**/*.jpg",
        "watchAssets": true,
        "outDir": "dist/apps/connexus-be"
      }
    ],
    "webpack": true,
    "webpackConfigPath": "webpack.config.js",
    "tsConfigPath": "apps/connexus-be/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/connexus-be",
  "projects": {
    "connexus-be": {
      "type": "application",
      "root": "apps/connexus-be",
      "entryFile": "main",
      "sourceRoot": "apps/connexus-be/src",
      "compilerOptions": {
        "tsConfigPath": "apps/connexus-be/tsconfig.app.json"
      }
    },
    "sow-rfq": {
      "type": "application",
      "root": "apps/sow-rfq",
      "entryFile": "main",
      "sourceRoot": "apps/sow-rfq/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sow-rfq/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "guards": {
      "type": "library",
      "root": "libs/guards",
      "entryFile": "index",
      "sourceRoot": "libs/guards/src",
      "compilerOptions": {
        "tsConfigPath": "libs/guards/tsconfig.lib.json"
      }
    },
    "ability": {
      "type": "library",
      "root": "libs/ability",
      "entryFile": "index",
      "sourceRoot": "libs/ability/src",
      "compilerOptions": {
        "tsConfigPath": "libs/ability/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "s3": {
      "type": "library",
      "root": "libs/s3",
      "entryFile": "index",
      "sourceRoot": "libs/s3/src",
      "compilerOptions": {
        "tsConfigPath": "libs/s3/tsconfig.lib.json"
      }
    },
    "background-jobs": {
      "type": "application",
      "root": "apps/background-jobs",
      "entryFile": "main",
      "sourceRoot": "apps/background-jobs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/background-jobs/tsconfig.app.json"
      }
    },
    "export-data": {
      "type": "library",
      "root": "libs/export-data",
      "entryFile": "index",
      "sourceRoot": "libs/export-data/src",
      "compilerOptions": {
        "tsConfigPath": "libs/export-data/tsconfig.lib.json"
      }
    },
    "health": {
      "type": "library",
      "root": "libs/health",
      "entryFile": "index",
      "sourceRoot": "libs/health/src",
      "compilerOptions": {
        "tsConfigPath": "libs/health/tsconfig.lib.json"
      }
    }
  }
}