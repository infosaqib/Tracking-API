# Environment
NODE_ENV=development

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/connexus_db?schema=public"
SHADOW_DATABASE_URL="postgresql://username:password@localhost:5432/connexus_shadow_db?schema=public"

# AWS Configuration for SQS
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# SQS Queue Configuration
EXPORT_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/your-account-id/export-queue

# Node.js Configuration
NODE_OPTIONS="--max-old-space-size=4096"
