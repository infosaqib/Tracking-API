{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "strict": true,
    "inlineSources": true,
    "resolveJsonModule": true,
    "sourceRoot": "/",
    "paths": {
      "src/*": [
        "apps/connexus-be/src/*"
      ],
      "@app/core": [
        "libs/core/src"
      ],
      "@app/core/*": [
        "libs/core/src/*"
      ],
      "@app/prisma": [
        "libs/prisma/src"
      ],
      "@app/prisma/*": [
        "libs/prisma/src/*"
      ],
      "@app/guards": [
        "libs/guards/src"
      ],
      "@app/guards/*": [
        "libs/guards/src/*"
      ],
      "@app/ability": [
        "libs/ability/src"
      ],
      "@app/ability/*": [
        "libs/ability/src/*"
      ],
      "@app/shared": [
        "libs/shared/src"
      ],
      "@app/shared/*": [
        "libs/shared/src/*"
      ],
      "@app/s3": [
        "libs/s3/src"
      ],
      "@app/s3/*": [
        "libs/s3/src/*"
      ],
      "@app/export-data": [
        "libs/export-data/src"
      ],
      "@app/export-data/*": [
        "libs/export-data/src/*"
      ],
      "@app/health": [
        "libs/health/src"
      ],
      "@app/health/*": [
        "libs/health/src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts"
  ]
}